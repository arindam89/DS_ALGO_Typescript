{
  "compilerOptions": {
    "module": "UMD",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "outDir": "dist/",
    "sourceMap": true,
    "declaration": true,
    "types": [
      "jest"
    ],
    "target": "ES2017",
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "dist",
    "__tests__"
  ]
}